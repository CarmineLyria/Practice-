package practice;

/**
 *
 * @author heroo
 */
public class Practice {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        
        {
            final int TIME_NEEDED = 1030;
            //defining integer
            String lizIsAnIdiot;
            //string name
            lizIsAnIdiot = "This is just practice.";
            //string output
            int timeOfDay = 1031;
            //integer for definition
            
            //flow of control for if
            if (timeOfDay > TIME_NEEDED){
                System.out.println("This is just a test." + lizIsAnIdiot + "You did fine.");
            
            //open control "else"
            } else {
                String fuckUp;
                fuckUp = "You fucked up.";
                                
                //declare and initialise loop counter
            
                int loopCounter = 0;
            
                //declare and initialise loop limit
            
                int loopLimit = 10;
            
                //loop begin
            
                while(loopCounter < loopLimit){
                loopCounter = loopCounter+1;
                System.out.println(fuckUp);
                } //close if/else
            }
        }
    }
    
}
